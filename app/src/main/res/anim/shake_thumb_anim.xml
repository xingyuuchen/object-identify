<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android">
    <rotate
        android:fromDegrees="0"
        android:toDegrees="4"
        android:pivotX="50%"
        android:pivotY="100%"
        android:duration="50"/>
    <rotate
        android:fromDegrees="4"
        android:toDegrees="-4"
        android:pivotX="50%"
        android:pivotY="100%"
        android:startOffset="50"
        android:duration="100"/>
    <rotate
        android:fromDegrees="-4"
        android:toDegrees="4"
        android:pivotX="50%"
        android:pivotY="100%"
        android:startOffset="150"
        android:duration="100"/>
    <rotate
        android:fromDegrees="4"
        android:toDegrees="-4"
        android:pivotX="50%"
        android:pivotY="100%"
        android:startOffset="250"
        android:duration="100"/>
    <rotate
        android:fromDegrees="-4"
        android:toDegrees="0"
        android:pivotX="50%"
        android:pivotY="100%"
        android:startOffset="350"
        android:duration="50"/>
</set>
